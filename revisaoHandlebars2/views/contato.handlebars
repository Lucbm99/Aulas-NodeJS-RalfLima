<form class="form-group">
    <label>Nome: </label>
    <input type="text" name="nome" placeholder="Digite seu nome..." class="form-group"><br>
    <label>Telefone: </label>
    <input type="text" name="telefone" placeholder="Digite seu telefone..." class="form-group"><br>
    <label>E-mail: </label>
    <input type="email" name="email" placeholder="Digite seu e-mail..." class="form-group"><br>
    <label>Cidade: </label>
    <input type="text" name="cidade" placeholder="Digite sua cidade..." class="form-group"><br>
    <label>Estado: </label>
    <input type="text" name="estado" placeholder="Digite seu estado..." class="form-group"><br>

    <button type="submit" class="btn btn-primary">Enviar</button>
</form>